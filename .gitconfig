[include]
	path = ~/.gitconfig_shared
[alias]
	rich-diff = "!f() { [ -z \"$GIT_PREFIX\" ] || cd \"$GIT_PREFIX\" && git diff --color \"$@\" | diff-so-fancy  | less --tabs=4 -RFX; }; f"
	graph = log --graph --decorate --pretty=oneline --abbrev-commit